{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "type",
    "name",
    "version",
    "main",
    "scripts",
    "keywords",
    "author",
    "license",
    "description",
    "dependencies",
    "devDependencies"
  ],
  "properties": {
    "type": {
      "type": "string",
      "default": "",
      "title": "The type Schema",
      "examples": [
        "module"
      ]
    },
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "docearte"
      ]
    },
    "version": {
      "type": "string",
      "default": "",
      "title": "The version Schema",
      "examples": [
        "1.0.0"
      ]
    },
    "main": {
      "type": "string",
      "default": "",
      "title": "The main Schema",
      "examples": [
        "index.js"
      ]
    },
    "scripts": {
      "type": "object",
      "default": {},
      "title": "The scripts Schema",
      "required": [
        "start",
        "test",
        "dev"
      ],
      "properties": {
        "start": {
          "type": "string",
          "default": "",
          "title": "The start Schema",
          "examples": [
            "http-server public"
          ]
        },
        "test": {
          "type": "string",
          "default": "",
          "title": "The test Schema",
          "examples": [
            "echo \"Error: no test specified\" \u0026\u0026 exit 1"
          ]
        },
        "dev": {
          "type": "string",
          "default": "",
          "title": "The dev Schema",
          "examples": [
            "tsc --watch"
          ]
        }
      },
      "examples": [
        {
          "start": "http-server public",
          "test": "echo \"Error: no test specified\" \u0026\u0026 exit 1",
          "dev": "tsc --watch"
        }
      ]
    },
    "keywords": {
      "type": "array",
      "default": [],
      "title": "The keywords Schema",
      "items": {},
      "examples": [
        []
      ]
    },
    "author": {
      "type": "string",
      "default": "",
      "title": "The author Schema",
      "examples": [
        ""
      ]
    },
    "license": {
      "type": "string",
      "default": "",
      "title": "The license Schema",
      "examples": [
        "ISC"
      ]
    },
    "description": {
      "type": "string",
      "default": "",
      "title": "The description Schema",
      "examples": [
        ""
      ]
    },
    "dependencies": {
      "type": "object",
      "default": {},
      "title": "The dependencies Schema",
      "required": [
        "typescript"
      ],
      "properties": {
        "typescript": {
          "type": "string",
          "default": "",
          "title": "The typescript Schema",
          "examples": [
            "^5.3.3"
          ]
        }
      },
      "examples": [
        {
          "typescript": "^5.3.3"
        }
      ]
    },
    "devDependencies": {
      "type": "object",
      "default": {},
      "title": "The devDependencies Schema",
      "required": [
        "http-server"
      ],
      "properties": {
        "http-server": {
          "type": "string",
          "default": "",
          "title": "The http-server Schema",
          "examples": [
            "^14.1.1"
          ]
        }
      },
      "examples": [
        {
          "http-server": "^14.1.1"
        }
      ]
    }
  },
  "examples": [
    {
      "type": "module",
      "name": "docearte",
      "version": "1.0.0",
      "main": "index.js",
      "scripts": {
        "start": "http-server public",
        "test": "echo \"Error: no test specified\" \u0026\u0026 exit 1",
        "dev": "tsc --watch"
      },
      "keywords": [],
      "author": "",
      "license": "ISC",
      "description": "",
      "dependencies": {
        "typescript": "^5.3.3"
      },
      "devDependencies": {
        "http-server": "^14.1.1"
      }
    }
  ]
}