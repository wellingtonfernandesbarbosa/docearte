{
  "name": "Doce Arte Confeitaria",
  "short_name": "docearte",
  "id": "/site.webmanifest",
  "start_url": "/",
  "description": "The Doce Arte Confectionery Home Page is an interactive platform for users to explore products, learn about the confectionery, access special offers, and order custom cakes. Built with HTML, CSS, and TypeScript.",
  "display": "standalone",
  "theme_color": "#431711",
  "background_color": "#431711",
  "display_override": [
    "fullscreen",
    "minimal-ui"
  ],
  "orientation": "any",
  "lang": "pt-BR",
  "icons": [
    {
      "src": "./src/assets/favicon/apple-touch-icon.png",
      "sizes": "180x180",
      "type": "image/png"
    },
    {
      "src": "./src/assets/favicon/android-chrome-192x192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "./src/assets/favicon/android-chrome-512x512.png",
      "type": "image/png",
      "sizes": "512x512"
    },
    {
      "src": "./src/assets/favicon/logo.png",
      "sizes": "511x511",
      "type": "image/png",
      "maskable": true
    }
  ],
  "screenshots": [
    {
      "src": "./src/assets/favicon/widescreen.png",
      "type": "image/png",
      "sizes": "1920x1080",
      "purpose": "any",
      "form_factor": "wide"
    },
    {
      "src": "./src/assets/favicon/portrait.png",
      "type": "image/png",
      "sizes": "433x857",
      "purpose": "any"
    }
  ]
}